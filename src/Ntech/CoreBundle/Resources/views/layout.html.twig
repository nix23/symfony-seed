{% spaceless %}

<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js"> <!--<![endif]-->
<head>
    {% block head %}
        <title>
            {% block title %}{% endblock %}
        </title>

        {% block meta %}
            <meta charset="utf-8">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="keywords" content="{% block keywords %}{% endblock %}">
            <meta name="description" content="{% block description %}{% endblock %}">
            <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
        {% endblock %}

        {% block stylesheets %}
            {% stylesheets filter='cssrewrite' output='css/*.css'
                'bundles/ntechcore/css/vendor/boilerplate.css'
                'bundles/ntechcore/css/vendor/normalize.css'
            %}
                <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
            {% endstylesheets %}
        {% endblock %}
    {% endblock %}
</head>

<body class="{% block bodyclass %}{% endblock %}">
    <!--[if lt IE 7]>
    <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser
        today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to
        better experience this site.</p>
    <![endif]-->

    {% block body %}
    {% endblock %}

    {% block javascripts %}
        {% javascripts output='js/*.js'
            '@NtechCoreBundle/Resources/public/js/vendor/namespace.js'
            '@NtechCoreBundle/Resources/public/js/vendor/jquery-1.10.2.min.js'
            '@NtechCoreBundle/Resources/public/js/vendor/modernizr-2.6.2.min.js'
            '@NtechCoreBundle/Resources/public/js/vendor/html5shiv.js'
            '@NtechCoreBundle/Resources/public/js/vendor/hoverOnTouchRemover.js'
            '@NtechCoreBundle/Resources/public/js/vendor/touchClickEvent.js'
        %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts output='js/*.js'
            'bundles/fosjsrouting/js/router.js'
            'js/fos_js_routes.js'
        %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</body>
</html>

{% endspaceless %}